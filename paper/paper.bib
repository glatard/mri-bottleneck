  keywords  = "Neuroimaging;Tool / Framework;My papers/mri-bottleneck",
  language  = "en",
  issn      = "1053-8119, 1095-9572",
  pmid      = "21757015",
  doi       = "10.1016/j.neuroimage.2011.06.078",
